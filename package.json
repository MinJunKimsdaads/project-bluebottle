{
  "name": "project-bluebottle",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "iconv-lite": "^0.7.0",
    "nodemon": "^3.1.11",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "zustand": "^5.0.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "electron ./public/electron.js",
    "dev": "concurrently \"npm run start\" \"npm run electron-dev\"",
    "electron-build": "npm run build && electron-builder",
    "backend-start": "node server/index.js",
    "backend-test": "nodemon server/index.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^39.2.1",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "publish": null,
    "icon": "public/icon.ico",
    "appId": "com.developkmj.Project_BlueBottle",
    "productName": "Project_BlueBottle",
    "artifactName": "Project_BlueBottle.${ext}",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "build/",
      "public/**/*",
      "node_modules/",
      "server/",
      "package.json"
    ],
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}
